{
  "ReverseProxy": {
    "Routes": {
      "auth-route": {
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/auth/{**catch-all}"
        }
      },
      "booking-route": {
        "ClusterId": "booking-cluster",
        "Match": {
          "Path": "/booking/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "auth-cluster": {
        "Destinations": {
          "auth-1": {
            "Address": "https://localhost:5001/"
          }
        }
      },
      "booking-cluster": {
        "Destinations": {
          "booking-1": {
            "Address": "https://localhost:6001/"
          },
          "booking-2": {
            "Address": "https://localhost:6002/"
          }
        }
      }
    }
  }
}
